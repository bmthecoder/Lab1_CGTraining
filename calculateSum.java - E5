package Lab1;
import java.util.*;
public class CalculateSum {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.println(calculateSum(n));
	}
	static int calculateSum(int a){
		int sum =0;
		for(int i=1;i<=a;i++){
			if(i%3==0 || i%5==0) sum+=i;
		}
		return sum;
	}

}
